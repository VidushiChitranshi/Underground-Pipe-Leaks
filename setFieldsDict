FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 1
    volScalarFieldValue alpha.particles 0
);

regions
(
    boxToCell
    {
        box ( -0.275 0 -0.01 ) ( 0.275 0.3 0.01 );
        fieldValues
        (
            volScalarFieldValue alpha.air 0.40
            volScalarFieldValue alpha.particles 0.60
        );
    }
);


